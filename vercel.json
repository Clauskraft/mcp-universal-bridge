{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "CLAUDE_MODEL": "claude-sonnet-4-5-20250929",
    "GEMINI_MODEL": "gemini-2.0-flash-exp",
    "OPENAI_MODEL": "gpt-4o",
    "API_TIMEOUT": "60000"
  }
}
